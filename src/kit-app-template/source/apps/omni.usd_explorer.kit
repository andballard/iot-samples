[settings]
renderer.multiGpu.enabled = false # do not enable Multi-GPU, will choose the "best" GPU
renderer.multiGpu.maxGpuCount = 1 # this ensures at most one GPU is enabled even if /persistent/renderer/multiGpu/enabled=true was saved in user.config.json
rtx-transient.meshlights.forceDisable = true # disable use mesh light sampling, not critical
rtx-transient.hydra.maxInstanceCount = 12000000 # limit the number of instances to 4 million to cap performance and memory impact of larger instance counts
rtx-transient.scenedb.useUniformsReindexing = true

# enable a more conservative approach to stopping loading of geometry when the amount of free GPU and system memory go below specified thresholds
rtx-transient.hydra.conservativeMemoryLimits = true

# Sentry (internal only, does not work outside VPN)
telemetry.enableSentry = true

# Default settings for Inertia
persistent.exts."omni.kit.manipulator.camera".tumbleSpeed = 720
persistent.exts."omni.kit.manipulator.camera".inertiaDecay = 3
persistent.exts."omni.kit.manipulator.prim".manipulator.placement = "Bounding Box Base"

# renderer.enabled = "rtx,iray,pxr"
# only enable rtx renderer by default
renderer.enabled = "rtx"  # ,pxr"
renderer.active = "rtx" # RTX As the default renderer always

# Sync materials in parallel from within Hydra
omnihydra.parallelHydraSprimSync = true
# Enable single Viewport camera model (setting w/ typo in current Kit)
ext."omni.kit.widget.viewport".sceneView.singleCameraModel.enabled = true

# Privacy
################################
privacy.externalBuild = false
structuredLog.enable = true

