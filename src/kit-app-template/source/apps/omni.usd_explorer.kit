# Change default stage template to what we have in the setup extension
newStage.defaultTemplate = "SunnySky"

viewport.camVelocityMin = 0.01
viewport.camVelocityMax = 50

[settings.app.viewport.defaultCamera.target.distance]
enabled = true
value = 5
units = "meters"

# Disable all HUD items by default
[settings.app.viewport.defaults.hud]
renderFPS.visible=false
renderResolution.visible=false
deviceMemory.visible=false
hostMemory.visible=false
renderProgress.visible=false

# SRT settings
camera.controllerUseSRT = true
# Misc Settings
###############################
viewport.snapToSurface = false
stage.movePrimInPlace = false
file.recentFiles = []
# Disable Frame Rate and Resoltion by default
viewport.displayOptions = 32767
transform.gizmoUseSRT = true

# Camera Inertia
viewport.camInertiaEnabled = false # Enabled
viewport.camInertiaAmount = 0.0 # Current default in Kit

# Make Detail panel visible by default
omniverse.content_browser.options_menu.show_details = true
omniverse.filepicker.options_menu.show_details = true

stage.instanceableOnCreatingReference = false

viewport.previewOnPeek = false

viewport.Viewport.Viewport0.fillViewport = false
viewport.Viewport.Viewport1.fillViewport = false

# 1154 = 0000 0000 0000 0000 0000 0100 1000 0010
# selection, mesh, axis default visible
# viewport.displayOptions = 1154
viewport.camVelocityMax = 20.0
file.initDialogWidth = 1200
file.initDialogHeight = 800
file.save.showUnsavedLayersDialog = true
captureFrame.viewport = true
omniverse.tooltipEnabled = false
# stage.movePrimInPlace = false
stage.upAxis = 'Z'
rendering.whiteModeExceptions = 'GizmoTex, Gizmo, OmniGlass, SunsetSkyMat, ProceduralSky, Blue_Glass, Clear_Glass, Dull_Glass, Frosted_Glass, Glazed_Glass, Green_Glass, Mirror, Red_Glass, Tinted_Glass, Tinted_Glass_R02, Tinted_Glass_R25, Tinted_Glass_R50, Tinted_Glass_R75, Tinted_Glass_R85, Tinted_Glass_R98, Cirrus, CumulusHeavy, CumulusLight, Overcast, NightSky'

# From omni.kit.viewport.bundle (check setup.py ::_on_stage_open_event)
primCreation.typedDefaults.camera.focalLength = 15


