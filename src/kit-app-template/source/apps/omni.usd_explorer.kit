# Windows only extensions. For all of them set exact=true to not be included into generated version lock.
# That will break linux otherwise, as version lock is platform agnostic currently.
[settings.app.extensions."filter:platform"."linux-x86_64"]
excluded = [
	"omni.kit.window.modifier.titlebar",
	"omni.rtx.shadercache.d3d12",
]

[dependencies."filter:platform"."windows-x86_64"]
"omni.kit.window.modifier.titlebar" = { version = "105.1.15", exact = true }
"omni.rtx.shadercache.d3d12" = {}

[settings.app]
name = "USD.Explorer"
versionFile = "${app}/../VERSION"
window.width = 1920
window.height = 1010
window.x = -1
window.y = -1
window.title = "USD Explorer"
window.fullscreen = false
window.minimalPanelsOnStartup = true
window.toolBarHorizontal = false
window.displayModeLock = false
window.showStatusBar = false
window.uiStyle = 'NvidiaDark'
window.iconPath = "${omni.usd_explorer.setup}/data/nvidia-omniverse-usd_explorer.ico"
window.imagePath = "${omni.usd_explorer.setup}/data/nvidia-omniverse-usd_explorer_about.png"
viewport.showSettingMenu = false
viewport.showCameraMenu = false
viewport.showRendererMenu = false
viewport.showHideMenu = false
renderer.skipWhileMinimized = true
renderer.resolution.width = 1920
renderer.resolution.height = 1080
docks.autoHideTabBar = false
docks.noWindowMenuButton = false
viewport.show.camera = false
viewport.show.lights = false
viewport.show.audio = false
viewport.grid.enabled = true
viewport.outline.enabled = true
viewport.boundingBoxes.hideWhenManipulating = true
viewport.offsets.navBottom = 0

"omni.usd".loadRenderSettingsFromUsdStage = false # ensure consistent settings across all USD stages independently of what was saved to them
# Enable parallel mode for selection performance
"omni.usd".getRenderablePaths.parallel = 4

# default to fill viewport
viewport.defaults.fillViewport = false

# Enable snap by default
viewport.snapEnabled = true

content.emptyStageOnStart = true
viewport.createCameraModelRep = true # Enable creation of camera meshes in USD
usdrt.scene_delegate.enableProxyCubes = false
# Disabling FSD geometry streaming for OMFP-2536
usdrt.scene_delegate.geometryStreaming.enabled = false
usdrt.scene_delegate.numFramesBetweenLoadBatches = 1
usdrt.scene_delegate.geometryStreaming.numberOfVerticesToLoadPerChunk = 800000
usdrt.scene_delegate.timeSampledDataStatic = true

# 0: Display everything
# 0.0005: Display all geometry down to around the size of a pixel
# 0.001: Display all geometry bigger than a few pixels
usdrt.scene_delegate.geometryStreaming.solidAngleLimit = 0

# When populating Fabric from USD, merge meshes under scene
# graph instances. Helps with e.g. Tesla
# Needs to be tested more before enabling for Apollo
usdrt.population.utils.mergeInstances = false

# Fast shutdown by default
fastShutdown = true

window.showStartup = true

# Turn on the Fabric scene delegate by default
useFabricSceneDelegate = true

# deprecate support for old kit.ui.menu
menu.legacy_mode = false

# Force enable Axis, Grid, Outline and Lights at startup
create.forceViewportSettings = true

# use omni.ui.Menu for the MenuBar
menu.compatibility_mode = false

# Disable MaterialX to ensure Material Watcher doesn't impact startup time
material.disableMtlx = true

# Hang Detector
################################
hangDetector.enabled = false
hangDetector.timeout = 120

# Tool State Management
tools.modal_tool_active = false

# Don't show Create->Shape menus
primCreation.hideShapes = true
primCreation.enableMenuAudio = false
primCreation.enableMenuCamera = false
liveSession.enableMenuFollowUser = false

# Expand viewport
startup.expandViewport = true


[settings.persistent.app]
stage.upAxis = 'Z'
viewport.camMoveVelocity = 1.5
viewport.pickingMode = "kind:model.ALL"
viewport.manipulator.camera.flyAcceleration = 1000.0
viewport.manipulator.camera.flyDampening = 10.0
viewport.manipulator.camera.lookAcceleration = 2000.0
viewport.manipulator.camera.lookDampening = 20.0
viewport.manipulator.camera.moveAcceleration = 1000.0
viewport.manipulator.camera.moveDampening = 10.0
viewport.manipulator.camera.tumbleAcceleration = 2000.0
viewport.manipulator.camera.tumbleDampening = 20.0
